-- player behavior

function create_player(id, db, x, y)
	return {
		id = id,
		x = x,
		y = y,
		dx = 0,
		dy = 0,
		speed = 1,
		state = 0,
		target = 0,
		candidate = 0,
		max_speed = 4,
		shake = 0,
		stand = {
			x = x,
			y = y,
			dx = 0,
			dy = 0,
			shake = 0,
			speed = speed[database[db].speed],
			max_speed = 100,
			flip = false,
			state = 0,
			target = 0,
			offset = 0
		},
		hp = 100,
		flip = false,
		offset = 0,
		db = database[db]
	}
end

function update_player(p)
	p.state = 0
	-- find target
	p.target = 0
	local distance = 128 * 128
	local hp = 100
	for other in all(players) do
		local curr = compute_distance(p, other)
		if (p.id != other.id and curr < distance) then
			distance = curr
			if (other.hp <= p.hp and other.hp < hp) then
				 p.candidate = other.id
				 hp = other.hp
			end
			if (curr <= range[p.db.range]) p.target = other.id
		end
	end
	update_physics(p)
end

function behavior_player(p)
	if p.id <= state.nb_player then
		control_player(p)
	else
		ia_player(p)
	end
end

function control_player(p)
	local d = {x = p.x, y = p.y}
	if (btn(‚¨áÔ∏è, p.id - 1)) d.y += 1
	if (btn(‚û°Ô∏è, p.id - 1)) d.x += 1
	if (btn(‚¨ÜÔ∏è, p.id - 1)) d.y -= 1
	if (btn(‚¨ÖÔ∏è, p.id - 1)) d.x -= 1
	move_to_point(p, d)
	if (btn(üÖæÔ∏è, p.id - 1)) activate_stand(p)
end


function activate_stand(p)
	if p.stand.state == 0 then
		p.stand.x = p.x
		p.stand.y = p.y
	end
	if p.target > 0 then
		p.stand.state = 1;
		p.stand.target = p.target;
	end
end

function callback_stand(p)
	if p.stand.state == 1 then
		p.stand.state = 2
		p.stand.target = p.id
	end
end

function deactivate_stand(p)
	p.stand.state = 0
	p.stand.target = 0
	p.stand.x = p.x
	p.stand.y = p.y
end

function attack_player_stand(p, opponent)
	p.flip = not opponent.stand.flip
	local xflip = (p.flip and -1 or 1)
	p.dx -= xflip * atk[opponent.db.atk] * 1.5
	p.hp = max(0, p.hp - atk[opponent.db.atk])
	p.shake = 1
	callback_stand(opponent)
end

function update_stand(p)
	if p.stand.state > 0 then
		local same_place = same_place(p.stand, players[p.stand.target])
		local distancePlayer = compute_distance(p.stand, p)
		if same_place then
			if p.stand.state == 1 then
				attack_player_stand(players[p.stand.target], p)
			else
				deactivate_stand(p)
			end
		elseif distancePlayer > range[p.db.range] and p.stand.state == 1 then
			callback_stand(p)
		else
			move_to_point(p.stand, players[p.stand.target])
			update_physics(p.stand)
		end
	end
end

