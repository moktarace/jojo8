-- jojo's bizarre adventures

function _init()
	players = {
		create_player(1, "josuke", 30, 30), 
		create_player(2, "jotaro", 40, 40),
		create_player(3, "dio", 70, 70)
	}		
	state = {
		nb_player = 1
	}
	lap = 0
	tick = 1
end

function _update()
	for p in all(players) do
		update_player(p)
		update_stand(p)
		behavior_player(p)
	end
	if ((abs(time()) % 0.5 == 0)) lap = (lap + 1) % 2
	if ((abs(time()) % 0.1 == 0)) tick *= -1
end

function _draw()
	cls(0)
	draw_map()
	for p in all(players) do
		draw_player(p)
	end
	for p in all(players) do
		draw_stand(p)
	end
	draw_ui(players[1])
end

