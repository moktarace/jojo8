-- player gfx

function draw_map()
	map(0,0,0,0,128,128)
end

function draw_player(p)
	draw_char(p, p.db.spr, 0)
end

function draw_stand(p)
	if (p.stand.state > 0) draw_char(p.stand, p.db.spr + 1, p.db.color)
end

function draw_char(p, s, c)
	local x, y = p.x + p.shake * rnd(1), p.y + p.shake * rnd(1)
	local move = 0
	if (p.dx != 0 or p.dy != 0) move = (lap * tick)
	rectfill(x + 1 - move, y + 8, x + 2 - move, y + 9, c)
	if (p.flip) then
		rectfill(x + 5 + move, y + 8, x + 6 + move, y + 9, c)
	else
		rectfill(x + 5 + move, y + 8, x + 6 + move, y + 9, c)
	end
	if (p.dx == 0 and p.dy == 0) y += p.offset
	pal({c, c, c, c, c, c, c, c, c, c, c, c, c, c, c, c})
	spr(s, x + 1, y, 1, 1, p.flip)
	spr(s, x - 1, y, 1, 1, p.flip)
	spr(s, x, y + 1, 1, 1, p.flip)
	spr(s, x, y - 1, 1, 1, p.flip)	
	pal()
	spr(s, x, y, 1, 1, p.flip)
end

function draw_ui(p)
	local camx, camy = p.x - 8 - 128 / 2 + p.shake * rnd(1), p.y - 8 - 128 / 2 + p.shake *  rnd(1)
	camera(camx, camy)
	rect(camx + 4,  camy + 4, camx + 6 + 50, camy + 8, 8)
	if (p.hp > 0) rectfill(camx + 5, camy + 5,  camx + 5 + (p.hp / 2), camy + 7, 8)
	if p.target > 0 then		
		local c = 0
		if (btn(üÖæÔ∏è, p.id - 1)) c = 7
		spr(255, players[p.target].x, players[p.target].y)
		print("press üÖæÔ∏è to punch", camx + 30 , camy + 120, c)
		if (not btn(üÖæÔ∏è, p.id - 1)) print("press üÖæÔ∏è to punch", camx + 30 - 1 , camy + 120 - 1, 2)
	end
end

